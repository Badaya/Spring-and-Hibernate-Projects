<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context 
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	
	<context:annotation-config />
 
	<bean id="foo" class="com.example.spring123.Foo">
		<property name="name" value="Abhinav"></property>
	</bean>
	<bean id="anotherFoo" class="com.example.spring123.Foo">
		<property name="name" value="Pandey"></property>
		
	</bean>
	<bean id="bar" class="com.example.spring123.Bar" />
</beans>